    "dev": "bun astro dev",
    "build": "bun astro build",
    "preview": "bun astro preview",
    "start": "bun dist/server/entry.mjs",
    "typecheck": "bun astro check --minimumSeverity error",
    "lint": "bun eslint --config ./eslint.config.mjs .",
    "clean": "rm -rf dist .astro",
    "drizzle-kit": "bun ./node_modules/drizzle-kit/bin.cjs",
    "db:generate": "bun ./node_modules/drizzle-kit/bin.cjs generate",
    "db:migrate": "bun ./node_modules/drizzle-kit/bin.cjs migrate",
    "db:seed": "bun src/db/seed.ts",
    "worker": "bun src/worker/standalone.ts",
    "storage:reset": "find storage -mindepth 1 ! -name '.keep' -exec rm -rf {} +",
    "storybook:build": "bun ./node_modules/storybook/dist/bin/index.js build",
    "maildev": "maildev -v --hide-extensions STARTTLS --mail-directory tmp/maildev/",
    "storybook": "bun ./node_modules/storybook/dist/bin/index.js dev -p 6006"
  },
